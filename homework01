#include <iostream>
using namespace std;
int numbersOfCard(long long int number, int power) {
	number %= static_cast<long long int>(pow(10, power));
	if (power > 1) {
		number /= static_cast<long long int>(pow(10, power - 1));
	}
	return number;
}
int main() {
	int controlNumber = 0, n = 0;
	cin >> n;
	long long int* numberOfCard = new long long int[n];
	for (int i = 0; i < n; i++) {
		cin >> numberOfCard[i];
	}
	for (int i = 0; i < n; i++) {
		for (int c = 2; c <= 13; c++) {
			if (c % 2 != 0) {
				controlNumber += numbersOfCard(numberOfCard[i], c);
			}
			else {
				controlNumber += numbersOfCard(numberOfCard[i], c) * 3;
			}
		}
		if ((10 - (controlNumber % 10)) == (numberOfCard[i] % 10)) {
			cout << 1 << endl;
		}
		else {
			cout << 0 << endl;
		}
		controlNumber = 0;
	}
	delete[] numberOfCard;
}
